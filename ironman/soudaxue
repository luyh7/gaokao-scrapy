  <script>
  $(function(){
    //Ajax发起请求，加载数据
    function getData(province,schooltype,page,size,keyWord1,schoolprop,schoolflag,schoolsort,schoolid){
      $.ajax({
          type:"GET",
          url:refLocation+"/soudaxue/queryschool.html?messtype=jsonp",
          dataType:"jsonp",
          data:{
            province:province,
            schooltype:schooltype,
            page:page,
            size:size,
            keyWord1:keyWord1,
            schoolprop:schoolprop,
            schoolflag:schoolflag,
            schoolsort:schoolsort,
            schoolid:schoolid
          },
          success:function(data){
            var schooldata=data.school;
            var totalRecordS = data.totalRecord.num;
            if(schooldata==undefined){
              var falsetxt='<tr><td colspan="7" style="color:'+'red;height:'+'100px">没有找到相关数据</td></tr>';
              $('.lin-seachtable').append(falsetxt);
            }else{
              //表格数据填充
              for(var i=0;i<schooldata.length;i++){
                var schoolname=schooldata[i].schoolname;
                var province=schooldata[i].province;
                var num=schooldata[i].num;
                var schooltype=schooldata[i].schooltype;
                var ranking=schooldata[i].ranking;
                if(ranking=='0'){
                	ranking='--';
                }else{
                	ranking=ranking;
                }
                var schoolproperty=schooldata[i].schoolproperty;
                var rankingCollegetype=schooldata[i].rankingCollegetype;
                if(rankingCollegetype=='0'){
                	rankingCollegetype='--';
                }else{
                	rankingCollegetype=rankingCollegetype;
                }
            	  var schoolid=schooldata[i].schoolid;
            	  var oldname=schooldata[i].oldname;
                var schoolurl='/schoolhtm/schoolTemple/school'+schoolid+'.htm';
                var addtxt='<tr class="lin-gettr"><td><a href=" '+schoolurl+'" target="_blank"  title="'+oldname+'">'+schoolname+'</a></td><td>'+province+'</td><!--<td>'+num+'</td>--><td>'+schooltype+'</td><td>'+ ranking +'</td><td>'+ schoolproperty+ ' ' + rankingCollegetype+'</td><td><input type="checkbox" /></td></tr>'
                $('.lin-seachtable').append(addtxt);
              }
              //分页数据
               SplitPage(totalRecordS,page,size);
            }
          },
      });
    }
getData(province,schooltype,page,30,keyWord1,schoolprop,schoolflag,null,null);    	
/*
    	*begin  广告位信息
    	*/
    	var isShowLeftAdParm=1;
    	var isShowRightAdParm=1;
    	  if(page==1){
    			getADZhaoDaXueMessage();//只首页调取
    		}else{
    			getRightAD("","","","","","","","","","","",isShowRightAd)//右侧默认
    		}
    	function  getADZhaoDaXueMessage(){
    		getLeftAD(province,schooltype,schoolprop,"queryschool",schoolflag,"","","","","",isShowLeftAdParm,isShowRightAdParm,keyWord1);
    		getRightAD(province,schooltype,schoolprop,"queryschool",schoolflag,"","","","","",isShowRightAdParm,isShowRightAd);
    	}
    	/*
    	*end
    	*/
  });
  </script>


